@model Natom.ATSA.Monitoreo.Models.Carga
@{
    ViewBag.Title = "Consiliación";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-md-8">
        <h2>Consiliación - Carga #@(Model.Numero.ToString().PadLeft(8, '0'))</h2>
    </div>
</div>


<div id="divConsiliacion"></div>

<div class="row">
    <ul class="nav nav-tabs">
        <li class="active"><a data-toggle="tab" href="#periodo">Facturas pendientes</a></li>
        <li><a data-toggle="tab" href="#auditados">Facturas consiliadas</a></li>
    </ul>
</div>

<input type="hidden" id="CargaId" value="@Model.CargaId" />

<div class="row">
    <div class="tab-content" style="padding: 10px 10px 0 10px;">
        <div id="periodo" class="tab-pane fade in active">
            <table width="100%" id="tblPendientes" class="table table-condensed">
                <thead>
                    <tr>
                        <th>
                            Orden
                        </th>
                        <th>
                            XX
                        </th>
                        <th>
                            IT
                        </th>
                        <th>
                            Afiliado
                        </th>
                        <th>
                            Apellido y Nombre
                        </th>
                        <th>
                            Practica
                        </th>
                        <th>
                            Can
                        </th>
                        <th>
                            Prestación
                        </th>
                        <th>
                            Fecha
                        </th>
                        <th>
                            Filial
                        </th>
                        <th></th>
                    </tr>
                </thead>
            </table>
        </div>
        <div id="auditados" class="tab-pane fade">
            <table width="100%" id="tblAuditados" class="table table-condensed">
                <thead>
                    <tr>
                        <th>
                            Orden
                        </th>
                        <th>
                            XX
                        </th>
                        <th>
                            IT
                        </th>
                        <th>
                            Afiliado
                        </th>
                        <th>
                            Apellido y Nombre
                        </th>
                        <th>
                            Practica
                        </th>
                        <th>
                            Can
                        </th>
                        <th>
                            Prestación
                        </th>
                        <th>
                            Fecha
                        </th>
                        <th>
                            Filial
                        </th>
                        <th></th>
                    </tr>
                </thead>
            </table>
        </div>
    </div>
</div>

<div id="ModalDiv">

</div>

<script src="~/Resources/js/auditorias/VerConsiliacion.min.js"></script>
